package com.demo.beans;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;

@Entity
public class Course {
	@Id
	@GeneratedValue
	private int cid;
	private String cname;
	private String fname;
	private String duration;
	private Faculty faculty;

	public Course() {
		super();
	}

	public Course(String cname, String fname, String duration, Faculty faculty) {
		super();
		this.cname = cname;
		this.fname = fname;
		this.duration = duration;
		this.faculty = faculty;
	}

	public Course(int cid, String cname, String fname, String duration, Faculty faculty) {
		super();
		this.cid = cid;
		this.cname = cname;
		this.fname = fname;
		this.duration = duration;
		this.faculty = faculty;
	}

	public int getCid() {
		return cid;
	}

	public void setCid(int cid) {
		this.cid = cid;
	}

	public String getCname() {
		return cname;
	}

	public void setCname(String cname) {
		this.cname = cname;
	}

	public String getFname() {
		return fname;
	}

	public void setFname(String fname) {
		this.fname = fname;
	}

	public String getDuration() {
		return duration;
	}

	public void setDuration(String duration) {
		this.duration = duration;
	}

	@Override
	public String toString() {
		return "Course [cid=" + cid + ", cname=" + cname + ", fname=" + fname + ", duration=" + duration + "]";
	}

}
