package com.demo.beans;

import javax.persistence.Entity;
import javax.persistence.FetchType;
import javax.persistence.Id;
import javax.persistence.OneToOne;

@Entity
public class Faculty {

	@Id
	private int fid;
	private String fname;
	private String spl;
	@OneToOne(fetch=FetchType.LAZY)
	private Course course;
	public Faculty() {
		super();
	}
	public Faculty(int fid, String fname, String spl, Course course) {
		super();
		this.fid = fid;
		this.fname = fname;
		this.spl = spl;
		this.course = course;
	}
	public Faculty(int fid, String fname, String spl) {
		super();
		this.fid = fid;
		this.fname = fname;
		this.spl = spl;
	}
	
	
	
}
